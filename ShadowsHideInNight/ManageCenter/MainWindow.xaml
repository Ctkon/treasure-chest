                                <Button x:Name="seaset" Width="40" Height="40" HorizontalAlignment="Right" BorderThickness="3" BorderBrush="#FF008DDE" Background="{x:Null}" Foreground="#FF008DDE" FontFamily="HoloLens MDL2 Assets" Content="&#xE71E;" FontSize="24"/>
                            </Grid>
                        </Grid>
                        <WrapPanel Margin="50,160,50,50">
                            <Border Width="320" Height="150">
                                <Button x:Name="gonor" Click="gonor_Click" Width="300" Height="130" BorderThickness="3" BorderBrush="#FF008DDE" Background="{x:Null}">
                                    <Grid Width="294">
                                        <Label Width="124" Height="124" HorizontalAlignment="Left" FontFamily="HoloLens MDL2 Assets" Foreground="#FF008DDE" Content="&#xEC7A;" FontSize="113.333"/>
