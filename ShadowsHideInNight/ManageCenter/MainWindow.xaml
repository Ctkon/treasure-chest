                                        </WrapPanel>
                                    </Grid>
                                </Button>
                            </Border>
                        </WrapPanel>
                    </Grid>
                    <!--主页-->
                    <Grid x:Name="shezhi" Background="White">
                        <Grid Height="60" VerticalAlignment="Top">
                            <Button x:Name="bacsethos" Click="bacsethos_Click" Width="60" Height="60" HorizontalAlignment="Left" VerticalAlignment="Center" Background="White" BorderThickness="0" FontFamily="HoloLens MDL2 Assets" Content="&#xE80F;" Foreground="#FF008DDE" FontSize="48"/>
