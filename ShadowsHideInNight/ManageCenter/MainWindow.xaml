                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                    <Setter TargetName="border" Property="Background" Value="#FFCBCBCB"/>
                                                                </Trigger>
                                                                <Trigger Property="IsPressed" Value="True">
                                                                    <Setter TargetName="border" Property="Background" Value="#FF646464"/>
                                                                </Trigger>
                                                            </ControlTemplate.Triggers>
                                                        </ControlTemplate>
                                                    </Button.Template>
                                                </Button>
                                            </Grid>
                                        </StackPanel>
                                    </ScrollViewer>
                                </Border>
                            </Grid>
                        </Grid>
                    </Grid>
                    <Button x:Name="cloappwin" Click="cloappwin_Click" Width="80" Height="60" Content="&#xE711;" HorizontalAlignment="Right" VerticalAlignment="Top" Background="#FF008DDE" BorderBrush="#FF008DDE" Foreground="White" FontFamily="HoloLens MDL2 Assets" FontSize="32"/>
                </Grid>
                <Grid x:Name="elcwin" Background="White" d:IsHidden="True"/>
                <Grid x:Name="powopt" Width="400" Height="150" Background="#FF2B2B2B" Opacity="0.8" Visibility="Hidden" AllowDrop="True">
                    <!--电源选项-->
                    <Label x:Name="powopt_titlebar" Height="40" VerticalAlignment="Top" Foreground="White" Content="电源选项" FontSize="22"/>
                    <Grid Width="240" Height="60">
                        <Button HorizontalAlignment="Left" x:Name="sleep" Click="sleep_Click" Width="60" Height="60" Style="{StaticResource BtRa}" Background="{x:Null}" BorderBrush="White" FontFamily="HoloLens MDL2 Assets" Content="&#xF11F;" Foreground="White" FontSize="28" ToolTip="睡眠"/>
                        <Button HorizontalAlignment="Center" x:Name="shutdown" Click="shutdown_Click" Width="60" Height="60" Style="{StaticResource BtRa}" Background="{x:Null}" BorderBrush="White" FontFamily="HoloLens MDL2 Assets" Content="&#xE7E8;" Foreground="White" FontSize="28" ToolTip="关机"/>
                        <Button HorizontalAlignment="Right" x:Name="restart" Click="restart_Click" Width="60" Height="60" Style="{StaticResource BtRa}" Background="{x:Null}" BorderBrush="White" FontFamily="HoloLens MDL2 Assets" Content="&#xE777;" Foreground="White" FontSize="28" ToolTip="重启"/>
                    </Grid>
                    <Button x:Name="powopt_cancel" Click="power_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="60" Height="40" Style="{StaticResource BtnInfoStyle}" Background="{x:Null}" BorderBrush="{x:Null}" Content="取消" Foreground="White" FontSize="22" ToolTip="电源选项"/>
                </Grid>
                <Grid x:Name="startMenu" Width="800" Height="600" HorizontalAlignment="Left" VerticalAlignment="Bottom" Opacity="0.8" Background="#FF2B2B2B" Visibility="Hidden">
                    <!--开始菜单-->
                    <Grid Width="40" HorizontalAlignment="Left" Margin="5,0">
                        <Image VerticalAlignment="Top" Width="40" Height="40" Source="Resources/Default/logimg.jpg" Stretch="Fill"/>
                        <WrapPanel Height="200" VerticalAlignment="Bottom">
                            <Button x:Name="Centerloud" Click="Centerloud_Click" VerticalAlignment="Bottom" Width="40" Height="40" Style="{StaticResource BtnInfoStyle}" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="HoloLens MDL2 Assets" Content="&#xEBD3;" Foreground="White" FontSize="22" ToolTip="启示云"/>
                            <Button x:Name="appman" Click="appman_Click" VerticalAlignment="Bottom" Width="40" Height="40" Style="{StaticResource BtnInfoStyle}" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="HoloLens MDL2 Assets" Content="&#xF120;" Foreground="White" FontSize="22" ToolTip="应用"/>
                            <Button x:Name="account" Click="account_Click" VerticalAlignment="Bottom" Width="40" Height="40" Style="{StaticResource BtnInfoStyle}" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="HoloLens MDL2 Assets" Content="&#xEA4A;" Foreground="White" FontSize="22" ToolTip="账户"/>
                            <Button x:Name="settings" Click="settings_Click" VerticalAlignment="Bottom" Width="40" Height="40" Style="{StaticResource BtnInfoStyle}" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="HoloLens MDL2 Assets" Content="&#xE713;" Foreground="White" FontSize="22" ToolTip="设置"/>
                            <Button x:Name="power" Click="power_Click" VerticalAlignment="Bottom" Width="40" Height="40" Style="{StaticResource BtnInfoStyle}" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="HoloLens MDL2 Assets" Content="&#xE7E8;" Foreground="White" FontSize="22" ToolTip="电源选项"/>
                        </WrapPanel>
                    </Grid>
                </Grid>
            </Grid>
            <Grid x:Name="taskBar" MouseDown="Window_DragMove" Background="#FF2B2B2B" Height="50" VerticalAlignment="Bottom" Margin="0,0,0,0" Opacity="0.8">
                <!--任务栏&开始按钮-->
                <Button x:Name="start" Click="start_Click" Margin="5,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="40" Height="40" Style="{StaticResource BtnInfoStyle}" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="HoloLens MDL2 Assets" Content="&#xE700;" Foreground="White" FontSize="22" ToolTip="开始菜单"/>
                <TextBlock x:Name="runtime" HorizontalAlignment="Right" VerticalAlignment="Top" Width="85" Height="40" Foreground="White" Text="17:30" FontSize="30"/>
                <WrapPanel x:Name="appsbar" Margin="45,0,0,10">

                </WrapPanel>
                <Rectangle Fill="White" Height="4" Margin="5,0,5,3" VerticalAlignment="Bottom" RadiusX="2" RadiusY="2"/>
            </Grid>
        </Grid>
    </Grid>
</Window>
