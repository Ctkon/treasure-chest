                                            <Label Width="167" Height="44" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Microsoft YaHei" Foreground="#FF008DDE" Content="常规" FontSize="26.667" FontWeight="Bold"/>
                                            <TextBlock Width="167" Height="80" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Microsoft YaHei" Foreground="#FF008DDE" Text="显示, 控制, 声音, 通知" FontSize="21.333" TextWrapping="WrapWithOverflow"/>
                                        </WrapPanel>
                                    </Grid>
                                </Button>
                            </Border>
                            <Border Width="320" Height="150">
                                <Button x:Name="gonet" Click="gonet_Click" Width="300" Height="130" BorderThickness="3" BorderBrush="#FF008DDE" Background="{x:Null}">
