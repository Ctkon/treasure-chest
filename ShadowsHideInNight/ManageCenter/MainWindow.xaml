                                                        <Label Content="&#xF11B;" FontFamily="HoloLens MDL2 Assets" HorizontalAlignment="Right" VerticalAlignment="Bottom" FontSize="200" Width="170"/>
                                                    </Grid>
                                                </WrapPanel>
                                                <WrapPanel x:Name="wbe" Width="700" Height="300" HorizontalAlignment="Center" VerticalAlignment="Center">
