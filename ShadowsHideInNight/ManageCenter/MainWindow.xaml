                                        <Label Width="124" Height="124" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="HoloLens MDL2 Assets" Foreground="#FF008DDE" Content="&#xE774;" FontSize="113.333"/>
                                        <WrapPanel Width="167" Height="124" HorizontalAlignment="Right">
                                            <Label Width="167" Height="44" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Microsoft YaHei" Foreground="#FF008DDE" Content="网络" FontSize="26.667" FontWeight="Bold"/>
