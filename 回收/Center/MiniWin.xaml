<pus:WindowX x:Class="Center.MiniWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Center"
        mc:Ignorable="d"
        xmlns:pus="clr-namespace:Panuon.UI.Silver;assembly=Panuon.UI.Silver"
        Title="伊莫特控制中心" MinHeight="450" MinWidth="200" Width="350" Height="600" FontSize="22">
    <pus:WindowX.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#FF3A3A3A" />
        </ResourceDictionary>
    </pus:WindowX.Resources>
    <Grid Margin="10">
        <Button Content="库" VerticalAlignment="Top" HorizontalAlignment="Right">
            <Button.ContextMenu>
                <ContextMenu>
                    <ContextMenu.Items>
                        <Label Content="库"/>
                        <Label Content="莫伊商城"/>
                        <Label Content="用户中心"/>
                        <Label Content="设置"/>
                    </ContextMenu.Items>
                </ContextMenu>
            </Button.ContextMenu>
        </Button>
        <Grid Margin="0,45,0,0">
            
        </Grid>
    </Grid>
</pus:WindowX>
