                                        <TextBlock FontSize="18" Text="{DynamicResource MainWindow_Shop}" Margin="0" TextAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                                    </Grid>
                                </Button>
