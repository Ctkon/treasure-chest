<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Catrol_Desktop"
    
        xmlns:md_controls="http://materialdesigninxaml.net/winfx/xaml/themes"
    
        xmlns:hd_controls="clr-namespace:HandyControl.Controls;assembly=HandyControl"
        xmlns:hc="https://handyorg.github.io/handycontrol" x:Class="Catrol_Desktop.MainWindow"
    
        xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
        
        xmlns:cefSharpWPF="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
    
        mc:Ignorable="d"
        x:Name="Window"
        WindowStyle="None" AllowsTransparency="True"
        Title="Catrol_Desktop" MinHeight="600" MinWidth="1000" Background="Transparent"
        Height="820" Width="1300">
    <Window.Resources>
        <hc:DoubleMinConverter x:Key="DoubleMinConverter"/>
        <SolidColorBrush x:Key="Frame_Background" Color="#00000000"/>
    </Window.Resources>
    <!--Height="620" Width="1020"-->
    <WindowChrome.WindowChrome>
        <WindowChrome ResizeBorderThickness="10" CaptionHeight="0"/>
    </WindowChrome.WindowChrome>
    <Grid x:Name="Container" Margin="6,8">
        <Grid.Effect>
            <DropShadowEffect ShadowDepth="0" BlurRadius="10"/>
        </Grid.Effect>
        <Border Background="#FF1E1E20" CornerRadius="5"/>
        <Grid>
            <Grid x:Name="Holder" Margin="40,40,0,0">
                <Grid x:Name="Frame_Holder">
                    <Frame x:Name="Frame" BorderBrush="{x:Null}" Background="{DynamicResource Frame_Background}"/>
                </Grid>
                <Border x:Name="Web_Holder_Container" CornerRadius="15,15,5,0" Height="0" VerticalAlignment="Bottom" Background="#FF262626">
                    <DockPanel>
                        <Border x:Name="Web_Holder_TitleBar" CornerRadius="15,15,0,0" Background="#FF3B3B3B" DockPanel.Dock="Top" Height="40">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0" Margin="25,0" VerticalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Button x:Name="Web_GoBacker" Content="&#xE830;" FontFamily="Segoe MDL2 Assets" Style="{StaticResource ButtonGroupItemHorizontalFirst}" Grid.Column="0" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}" BorderBrush="{x:Null}" Foreground="{DynamicResource {x:Static SystemColors.MenuHighlightBrushKey}}"/>
                                    <Button x:Name="Web_Refresher" Content="&#xE149;" FontFamily="Segoe MDL2 Assets" Style="{StaticResource ButtonGroupItemDefault}" Grid.Column="1" BorderBrush="{x:Null}" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                                    <Button x:Name="Web_GoForwarder" Content="&#xEA47;" FontFamily="Segoe MDL2 Assets" Style="{StaticResource ButtonGroupItemHorizontalLast}" Grid.Column="2" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}" BorderBrush="{x:Null}" Foreground="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                                </Grid>
                                <Label x:Name="Web_Title" Content="新标签页" FontSize="24" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="1" Foreground="#DDFFFFFF"/>

                                <Grid Grid.Column="4" Margin="10,0" VerticalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="5"/>
                                    </Grid.ColumnDefinitions>
                                    <Button x:Name="Web_Shrinker" Content="&#xE1FD;" Grid.Column="0" Style="{StaticResource MaterialDesignOutlinedButton}" FontFamily="Segoe MDL2 Assets" BorderBrush="{DynamicResource LinearBlue}" Foreground="{DynamicResource LinearBlue}"/>
                                    <Button x:Name="Web_ToNewer" Content="&#xE72D;" Grid.Column="2" Style="{StaticResource MaterialDesignOutlinedButton}" FontFamily="Segoe MDL2 Assets" BorderBrush="{DynamicResource PrimaryHueDarkBrush}" Foreground="{DynamicResource PrimaryHueDarkBrush}"/>
                                    <Button x:Name="Web_Clearer" Content="&#xE8BB;" Grid.Column="4" Style="{StaticResource MaterialDesignOutlinedButton}" FontFamily="Segoe MDL2 Assets" BorderBrush="#FFFF5B2B" Foreground="#FFFF5B2B"/>
                                </Grid>
                            </Grid>
                        </Border>
                        <Border DockPanel.Dock="Bottom" CornerRadius="0,0,5,0" Height="15" Background="#FF3B3B3B">

                        </Border>
                        <DockPanel>
                            <!--<Rectangle Width="15" DockPanel.Dock="Left" Fill="#FF3B3B3B"/>
                            <Rectangle Width="15" DockPanel.Dock="Right" Fill="#FF3B3B3B"/>-->
                            <Border x:Name="Web_Holder">

                            </Border>
                        </DockPanel>
                    </DockPanel>
                </Border>
                <Canvas x:Name="Character_Holder" Width="200" Height="200" Margin="0,0,40,0" HorizontalAlignment="Right" VerticalAlignment="Bottom">

                    <!--<svgc:SvgViewbox x:Name="Character" Width="170" Height="170" Canvas.Top="170" Canvas.Left="15" Source="pack://application:,,,/Sources/Face With Monocle.svg"/>
                    <Rectangle x:Name="Character_Eye" Canvas.Top="95" Canvas.Left="122" Width="15" Height="20" Fill="Black" RadiusY="7.5" RadiusX="7.5"/>
                    <Ellipse x:Name="Character_Eye_Center" Canvas.Top="109" Canvas.Left="129" Width="1" Height="1" Fill="Black"/>-->

                </Canvas>
            </Grid>
            <Border x:Name="Chrome_TitleBar" MouseDown="TitleBar_MouseDown" CornerRadius="5,5,0,0" Background="#FF1E1E20" Height="42" VerticalAlignment="Top">
                <Grid>
                    <DockPanel Height="40" VerticalAlignment="Top">
                        <Grid Width="505" Margin="43,0,0,0" DockPanel.Dock="Left">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="5"/>
                                <ColumnDefinition Width="6.7*"/>
                            </Grid.ColumnDefinitions>
                            <Button x:Name="GoBacker" Content="&#xE830;" FontFamily="Segoe MDL2 Assets" Style="{StaticResource ButtonGroupItemHorizontalFirst}" Grid.Column="0" Background="{DynamicResource PrimaryHueLightBrush}" BorderBrush="{x:Null}" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                            <Button x:Name="Refresher" Content="&#xE149;" FontFamily="Segoe MDL2 Assets" Style="{StaticResource ButtonGroupItemDefault}" Grid.Column="1" BorderBrush="{x:Null}" Background="{DynamicResource PrimaryHueLightBrush}" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                            <Button x:Name="GoForwarder" Content="&#xEA47;" FontFamily="Segoe MDL2 Assets" Style="{StaticResource ButtonGroupItemHorizontalLast}" Grid.Column="2" Background="{DynamicResource PrimaryHueLightBrush}" BorderBrush="{x:Null}" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                            <hd_controls:SearchBar Margin="0,5" Grid.Column="4" SelectionBrush="{DynamicResource PrimaryHueMidBrush}" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}" Background="{DynamicResource PrimaryHueLightBrush}" SelectionTextBrush="{DynamicResource PrimaryHueMidForegroundBrush}" ShowClearButton="True"/>
                        </Grid>
                        <WrapPanel VerticalAlignment="Center" DockPanel.Dock="Right">
                            <Grid Width="240">
                                <Grid VerticalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Button x:Name="GoMessageWin" Grid.Column="0" Content="&#xE15F;" Style="{StaticResource MaterialDesignFlatDarkBgButton}" FontFamily="Segoe MDL2 Assets"/>
                                    <Button x:Name="Personalizer" Grid.Column="2" Content="&#xE771;" Style="{StaticResource MaterialDesignFlatAccentBgButton}" FontFamily="Segoe MDL2 Assets"/>
                                    <Button x:Name="Per" Grid.Column="4" Content="&#xE118;" Style="{StaticResource MaterialDesignFlatMidBgButton}" FontFamily="Segoe MDL2 Assets"/>
                                </Grid>
                                <Popup x:Name="MessagePopupBox" StaysOpen="False" Width="240" Height="545" VerticalAlignment="Bottom" HorizontalAlignment="Center">
                                    <DockPanel Background="#FF2D2D30">
                                        <StackPanel DockPanel.Dock="Top">
                                            <Label Style="{x:Null}"/>
                                            <Grid>
                                                <Button x:Name="MessagePopupBox_Hider" Content="&#xE70E;" Width="40" Height="30" FontSize="18" FontFamily="Segoe MDL2 Assets" Margin="20,0,0,0" Style="{StaticResource MaterialDesignToolForegroundButton}" HorizontalAlignment="Left" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                <Button x:Name="ViewMessagesInWeb" Content="&#xE92D;" Margin="5,0" Style="{StaticResource MaterialDesignOutlinedButton}" FontFamily="Segoe MDL2 Assets" HorizontalAlignment="Right"/>
                                            </Grid>
                                            <Label Style="{x:Null}"/>
                                            <Label Style="{StaticResource LabelInfo}" Content="{DynamicResource MainWindow_MessageCenter}"/>
                                            <Label Style="{x:Null}"/>
                                        </StackPanel>
                                        <ScrollViewer Style="{StaticResource ScrollViewerNativeBaseStyle}" VerticalScrollBarVisibility="Auto">
                                            <StackPanel x:Name="Messages_Holder" Margin="5,0,10,0">

                                            </StackPanel>
                                        </ScrollViewer>
                                    </DockPanel>
                                </Popup>
                                <Popup x:Name="PersonalizePopupBox" StaysOpen="False" Width="240" Height="545" VerticalAlignment="Bottom" HorizontalAlignment="Center">
                                    <DockPanel Background="#FF2D2D30">
                                        <StackPanel DockPanel.Dock="Top">
                                            <Label Style="{x:Null}"/>
                                            <Grid>
                                                <Button Content="&#xE70E;" Width="40" Height="30" FontSize="18" FontFamily="Segoe MDL2 Assets" Style="{StaticResource MaterialDesignToolForegroundButton}" HorizontalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                            </Grid>
                                            <Label Style="{x:Null}"/>
                                            <Label Style="{StaticResource LabelInfo}" Content="{DynamicResource MainWindow_UIEditor_Title}"/>
                                            <Label Style="{x:Null}"/>
                                        </StackPanel>
                                        <ScrollViewer Style="{StaticResource ScrollViewerNativeBaseStyle}" VerticalScrollBarVisibility="Auto">
                                            <StackPanel Margin="5,0,10,0">
                                                <DockPanel>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_Opacity}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <Slider Maximum="1" Value="{Binding Opacity, ElementName=Container, Mode=TwoWay}" SmallChange="0.01"/>
                                                </DockPanel>
                                                <Label Style="{x:Null}"/>
                                                <DockPanel>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_Width}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <hc:NumericUpDown Value="{Binding Width, ElementName=Window, Mode=TwoWay}" Background="{x:Null}" BorderBrush="{DynamicResource PrimaryHueMidBrush}" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                </DockPanel>
                                                <Label Style="{x:Null}"/>
                                                <DockPanel>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_Height}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <hc:NumericUpDown Value="{Binding ElementName=Window, Path=Height, Mode=TwoWay}" Background="{x:Null}" BorderBrush="{DynamicResource PrimaryHueMidBrush}" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                </DockPanel>
                                                <Label Style="{x:Null}"/>
                                                <DockPanel>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_Top}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <hc:NumericUpDown Value="{Binding Top, ElementName=Window, Mode=TwoWay}" Background="{x:Null}" BorderBrush="{DynamicResource PrimaryHueMidBrush}" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                </DockPanel>
                                                <Label Style="{x:Null}"/>
                                                <DockPanel>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_Left}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <hc:NumericUpDown Value="{Binding ElementName=Window, Path=Left, Mode=TwoWay}" Background="{x:Null}" BorderBrush="{DynamicResource PrimaryHueMidBrush}" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                </DockPanel>
                                                <Label Style="{x:Null}"/>
                                                <DockPanel>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_ContrastColor_EnableCheckBox}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <CheckBox x:Name="UI_ContraryColor_CheckBox" VerticalAlignment="Center" Content="{Binding IsChecked, ElementName=UI_ContraryColor_CheckBox}" Style="{StaticResource MaterialDesignCheckBox}" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                </DockPanel>
                                                <Label Style="{x:Null}"/>
                                                <DockPanel>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_3DCharacter_Enable_CheckBox}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <CheckBox x:Name="UI_3DCharacter_Enable_CheckBox" VerticalAlignment="Center" Content="{Binding IsChecked, ElementName=UI_3DCharacter_Enable_CheckBox}" Style="{StaticResource MaterialDesignCheckBox}" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                </DockPanel>
                                                <Label Style="{x:Null}"/>
                                                <DockPanel>
                                                    <TextBlock Text="&#xE12B;" FontFamily="Segoe MDL2 Assets" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <TextBlock Text="{DynamicResource MainWindow_UIEditor_Language}" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                                                    <ComboBox x:Name="UI_Language_Selecter" SelectedIndex="0" Style="{StaticResource MaterialDesignComboBox}" VerticalContentAlignment="Center" Foreground="{DynamicResource PrimaryHueMidBrush}" BorderBrush="{DynamicResource PrimaryHueMidBrush}">
                                                        <ComboBoxItem Content="简体中文" Background="White"/>
                                                        <!--汉语-->
                                                        <ComboBoxItem Content="English(American)"/>
                                                        <!--英语-->
                                                        <ComboBoxItem Content="わご(Translating)"/>
                                                        <!--日语-->
                                                        <ComboBoxItem Content="Français(Translating)"/>
                                                        <!--法语-->
                                                        <ComboBoxItem Content="한국어(Translating)"/>
                                                        <!--韩语-->
                                                        <ComboBoxItem Content="Español(Translating)"/>
                                                        <!--西班牙语-->
                                                    </ComboBox>
                                                </DockPanel>
                                            </StackPanel>
                                        </ScrollViewer>
                                    </DockPanel>
                                </Popup>
                            </Grid>
                            <Label Style="{x:Null}"/>
                            <Grid Width="165" HorizontalAlignment="Right">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="5"/>
                                </Grid.ColumnDefinitions>
                                <Button x:Name="Chrome_Minimizer" Content="&#xE921;" Grid.Column="0" Style="{StaticResource MaterialDesignOutlinedButton}" FontFamily="Segoe MDL2 Assets"/>
                                <Button x:Name="Chrome_MaximizerOrRestorer" Content="&#xE922;" Grid.Column="2" Style="{StaticResource MaterialDesignOutlinedButton}" FontFamily="Segoe MDL2 Assets"/>
                                <Button x:Name="Chrome_Closer" Content="&#xE8BB;" Grid.Column="4" Style="{StaticResource MaterialDesignOutlinedButton}" FontFamily="Segoe MDL2 Assets" BorderBrush="{DynamicResource PrimaryHueLightForegroundBrush}" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                            </Grid>
                        </WrapPanel>
                        <DockPanel>
                            <Label Style="{x:Null}"/>
                            <Button x:Name="Web_Expander" Content="&#xF847;" Style="{StaticResource MaterialDesignToolForegroundButton}" FontFamily="Segoe MDL2 Assets" Foreground="{DynamicResource LinearBlue}"/>
                            <Label Style="{x:Null}"/>
                        </DockPanel>
                    </DockPanel>
                    <Rectangle Height="2" Margin="3,40,3,0" Fill="#FF343436" VerticalAlignment="Bottom" RadiusY="1" RadiusX="1"/>
                </Grid>
            </Border>
            <Border x:Name="Chrome_Navigater" Width="40" CornerRadius="5,0,0,5" HorizontalAlignment="Left" Background="#FF171718">
                <!--<ScrollViewer Margin="0,80,0,0" VerticalScrollBarVisibility="Auto">
                    <StackPanel>

                    </StackPanel>
                </ScrollViewer>-->
                <Grid>
                    <DockPanel>
                        <Grid DockPanel.Dock="Top">
                            <Image Height="80" Width="40" VerticalAlignment="Top" HorizontalAlignment="Left"/>
                            <Image Height="80" Width="160" VerticalAlignment="Top" Margin="40,0,0,0"/>
                        </Grid>
                        <DockPanel>
                            <StackPanel DockPanel.Dock="Top">
                                <Button x:Name="Go_Home" Width="150" Height="45" Style="{StaticResource MaterialDesignToolForegroundButton}">
                                    <Grid Width="150" Height="30">
                                        <Path Margin="5" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Center" Data="F1 M 10 1.611328 L 19.189453 10.810547 L 18.310547 11.689453 L 17.5 10.888672 L 17.5 18.75 L 11.25 18.75 L 11.25 12.5 L 8.75 12.5 L 8.75 18.75 L 2.5 18.75 L 2.5 10.888672 L 1.689453 11.689453 L 0.810547 10.810547 Z M 16.25 17.5 L 16.25 9.638672 L 10 3.388672 L 3.75 9.638672 L 3.75 17.5 L 7.5 17.5 L 7.5 11.25 L 12.5 11.25 L 12.5 17.5 Z " Fill="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                                        <TextBlock FontSize="18" Text="{DynamicResource MainWindow_Home}" Margin="0" TextAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                                    </Grid>
                                </Button>
                                <Button x:Name="Go_Library" Width="150" Height="45" Style="{StaticResource MaterialDesignToolForegroundButton}">
                                    <Grid Width="150" Height="30">
                                        <Path Margin="5" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Center" Data="F1 M 6.25 17.5 L 6.25 2.5 L 10 2.5 L 10 17.5 Z M 7.5 3.75 L 7.5 16.25 L 8.75 16.25 L 8.75 3.75 Z M 1.25 17.5 L 1.25 2.5 L 5 2.5 L 5 17.5 Z M 2.5 3.75 L 2.5 16.25 L 3.75 16.25 L 3.75 3.75 Z M 14.560547 2.275391 L 19.287109 16.435547 L 15.908203 17.568359 L 11.181641 3.408203 Z M 12.568359 4.091797 L 16.591797 16.181641 L 17.900391 15.751953 L 13.867188 3.662109 Z " Fill="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                                        <TextBlock FontSize="18" Text="{DynamicResource MainWindow_Lib}" Margin="0" TextAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"/>
                                    </Grid>
                                </Button>
                                <Button x:Name="Go_Shop" Width="150" Height="45" Style="{StaticResource MaterialDesignToolForegroundButton}">
                                    <Grid Width="150" Height="30">
                                        <Path Margin="5" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Center" Data="F1M17.5,5L17.5,17.5C17.5,17.845053 17.433268,18.168945 17.299805,18.47168 17.16634,18.774414 16.987305,19.038086 16.762695,19.262695 16.538086,19.487305 16.272785,19.666342 15.966797,19.799805 15.660807,19.933268 15.338541,20 15,20L3.75,20C3.404948,20 3.081055,19.933268 2.77832,19.799805 2.475586,19.666342 2.211914,19.487305 1.987305,19.262695 1.762695,19.038086 1.583659,18.774414 1.450195,18.47168 1.316732,18.168945 1.25,17.845053 1.25,17.5L1.25,5 3.75,5 3.75,3.75C3.75,3.235678 3.849284,2.750652 4.047852,2.294922 4.246419,1.839193 4.514974,1.442059 4.853516,1.103516 5.192057,0.764975 5.589192,0.49642 6.044922,0.297852 6.500651,0.099285 6.985677,0 7.5,0 8.177083,0 8.805338,0.169271 9.384766,0.507813 9.964192,0.169271 10.585938,0 11.25,0 11.764322,0 12.249349,0.099285 12.705078,0.297852 13.160807,0.49642 13.557941,0.764975 13.896484,1.103516 14.235025,1.442059 14.50358,1.839193 14.702148,2.294922 14.900715,2.750652 14.999999,3.235678 15,3.75L15,5z M13.75,3.75C13.75,3.411459 13.683268,3.089193 13.549805,2.783203 13.416341,2.477215 13.237305,2.211914 13.012695,1.987305 12.788086,1.762695 12.522786,1.58366 12.216797,1.450195 11.910807,1.316732 11.588541,1.25 11.25,1.25 10.957031,1.25 10.680338,1.298828 10.419922,1.396484 10.608724,1.630859 10.758463,1.86198 10.869141,2.089844 10.979817,2.317709 11.062825,2.545574 11.118164,2.773438 11.173502,3.001303 11.20931,3.234051 11.225586,3.47168 11.241861,3.709311 11.25,3.958334 11.25,4.21875L11.25,5 13.75,5z M5,5L10,5 10,3.75C10,3.411459 9.933268,3.089193 9.799805,2.783203 9.666341,2.477215 9.487305,2.211914 9.262695,1.987305 9.038086,1.762695 8.772786,1.58366 8.466797,1.450195 8.160807,1.316732 7.838542,1.25 7.5,1.25 7.154948,1.25 6.831055,1.316732 6.52832,1.450195 6.225586,1.58366 5.961914,1.762695 5.737305,1.987305 5.512695,2.211914 5.333659,2.477215 5.200195,2.783203 5.066731,3.089193 5,