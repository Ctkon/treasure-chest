        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MFC_Server"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:handyControl="clr-namespace:HandyControl.Controls;assembly=HandyControl"
        x:Name="mainWindow"
        xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
        Title="MFC-Server" MinHeight="720" MinWidth="1100" Height="850" Width="1350" AllowsTransparency="True" WindowStyle="None" Background="{x:Null}"
        Topmost="{Binding ElementName=ToggleButton_IsTop, Path=IsChecked}">
    <WindowChrome.WindowChrome>
        <WindowChrome ResizeBorderThickness="10" CaptionHeight="0"/>
    </WindowChrome.WindowChrome>
    <Window.Resources>
    </Window.Resources>
    <Grid Margin="6,8">
        <Border CornerRadius="5" Background="{DynamicResource MaterialDesignBackground}">
            <Border.Effect>
                <DropShadowEffect ShadowDepth="0" BlurRadius="10"/>
            </Border.Effect>
        </Border>
        <materialDesign:DrawerHost x:Name="DrawerHost" LeftDrawerBackground="{x:Null}">
            <materialDesign:DrawerHost.LeftDrawerContent>
                <Border CornerRadius="5" Background="{DynamicResource MaterialDesignBackground}" MouseDown="TitleBar_MouseDown">
                    <ScrollViewer Style="{DynamicResource ListViewScrollViewerStyle}">
                        <StackPanel Margin="16" MinWidth="200">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="8*"/>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="2*"/>
                                </Grid.ColumnDefinitions>
                                <Label FontSize="28" Content="{DynamicResource Menu_Title}" Grid.Column="0" Style="{StaticResource MaterialDesignLabel}"/>
                                <ToggleButton Grid.Column="2" Foreground="{DynamicResource PrimaryHueMidForegroundBrush}" IsChecked="{Binding ElementName=DrawerHost, Path=IsLeftDrawerOpen}" Style="{DynamicResource MaterialDesignHamburgerToggleButton}" Command="{x:Static materialDesign:DrawerHost.CloseDrawerCommand}" CommandParameter="{x:Static Dock.Left}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                            <TreeView>
                                <TreeViewItem x:Name="Persons" IsExpanded="True">
                                    <TreeViewItem.Header>
                                        <DockPanel>
                                            <Image Width="22" Height="22" Source="Data/Icon/Persons.png" />
                                            <TextBlock Margin="8,0" FontSize="28" Text="{DynamicResource Menu_Item_Persons}"/>
                                        </DockPanel>
                                    </TreeViewItem.Header>

                                    <TreeViewItem x:Name="Workers">
                                        <TreeViewItem.Header>
                                            <DockPanel>
                                                <Image Width="22" Height="22" Source="Data/Icon/Teacher.png" />
                                                <TextBlock Margin="8,0" FontSize="20" Text="{DynamicResource Menu_Item_Persons_Workers}"/>
                                            </DockPanel>
                                        </TreeViewItem.Header>
                                    </TreeViewItem>
                                    <TreeViewItem x:Name="Student">
                                        <TreeViewItem.Header>
                                            <DockPanel>
                                                <Image Width="22" Height="22" Source="Data/Icon/Student.png" />
                                                <TextBlock Margin="8,0" FontSize="20" Text="{DynamicResource Menu_Item_Persons_Students}"/>
                                            </DockPanel>
                                        </TreeViewItem.Header>
                                    </TreeViewItem>

                                </TreeViewItem>
                                <TreeViewItem x:Name="Debit" IsExpanded="True">
                                    <TreeViewItem.Header>
                                        <DockPanel>
                                            <Image Width="22" Height="22" Source="Data/Icon/Debit.png" />
                                            <TextBlock Margin="8,0" FontSize="28" Text="{DynamicResource Menu_Item_Debit}"/>
                                        </DockPanel>
                                    </TreeViewItem.Header>

                                    <TreeViewItem x:Name="Library">
                                        <TreeViewItem.Header>
                                            <DockPanel>
                                                <Image Width="22" Height="22" Source="Data/Icon/Library.png" />
                                                <TextBlock Margin="8,0" FontSize="20" Text="{DynamicResource Menu_Item_Debit_Library}"/>
                                            </DockPanel>
                                        </TreeViewItem.Header>
                                    </TreeViewItem>

                                </TreeViewItem>
                                <TreeViewItem x:Name="Finance" IsExpanded="True">
                                    <TreeViewItem.Header>
                                        <DockPanel>
                                            <Image Width="22" Height="22" Source="Data/Icon/Finance.png" />
                                            <TextBlock Margin="8,0" FontSize="28" Text="{DynamicResource Menu_Item_Finance}"/>
                                        </DockPanel>
                                    </TreeViewItem.Header>

                                    <TreeViewItem x:Name="Account">
                                        <TreeViewItem.Header>
                                            <DockPanel>
                                                <Image Width="22" Height="22" Source="Data/Icon/Account.png" />
                                                <TextBlock Margin="8,0" FontSize="20" Text="{DynamicResource Menu_Item_Finance_Account}"/>
                                            </DockPanel>
                                        </TreeViewItem.Header>
                                    </TreeViewItem>
                                    <TreeViewItem x:Name="Restaurant">
                                        <TreeViewItem.Header>
                                            <DockPanel>
                                                <Image Width="22" Height="22" Source="Data/Icon/Restaurant.png" />
                                                <TextBlock Margin="8,0" FontSize="20" Text="{DynamicResource Menu_Item_Finance_Restaurant}"/>
                                            </DockPanel>
                                        </TreeViewItem.Header>
                                    </TreeViewItem>

                                </TreeViewItem>
                                <TreeViewItem x:Name="CCTV" IsExpanded="True">
                                    <TreeViewItem.Header>
                                        <DockPanel>
                                            <Image Width="22" Height="22" Source="Data/Icon/CCTV.png" />
                                            <TextBlock Margin="8,0" FontSize="20" Text="{DynamicResource Menu_Item_CCTV}"/>
                                        </DockPanel>
                                    </TreeViewItem.Header>

                                </TreeViewItem>
                            </TreeView>
                        </StackPanel>
                    </ScrollViewer>
